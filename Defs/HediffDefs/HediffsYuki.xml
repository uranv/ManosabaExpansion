<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <HediffDef>
        <defName>UmHediffYukiDummy</defName>
        <label>Yuki Tsukishiro did this</label>
        <description>Manages special health states for Yuki Tsukishiro. You should not be seeing this.</description>
        <hediffClass>HediffWithComps</hediffClass>
        <defaultLabelColor>(199,213,231)</defaultLabelColor>
        <isBad>false</isBad>
        <everCurableByItem>false</everCurableByItem>
        <stages>
            <li>
                <becomeVisible>false</becomeVisible>
            </li>
        </stages>
        <comps>
            <li Class="UranvManosaba.Contents.Comps.HediffCompProperties_YukiDummy">
                <visitorHediff>UmHediffYukiVisitor</visitorHediff>
                <visitorHiddenHediff>UmHediffYukiVisitorHidden</visitorHiddenHediff>
                <colonistHediff>UmHediffYukiColonist</colonistHediff>
                <colonistHiddenHediff>UmHediffYukiColonistHidden</colonistHiddenHediff>
            </li>
            <li Class="UranvManosaba.Contents.Comps.HediffCompProperties_NarehateHealing">
                <applyPostMultiplier>true</applyPostMultiplier>
                <applyStatusSwitch>false</applyStatusSwitch>
            </li>
            <li Class="HediffCompProperties_GiveAbility">
                <abilityDefs>
                    <li>UmAbilityYukiAdd</li>
                    <li>UmAbilityYukiRemove</li>
                    <li>UmAbilityYukiSkip</li>
                </abilityDefs>
            </li>
        </comps>
    </HediffDef>
    <HediffDef>
        <defName>UmHediffYukiVisitor</defName>
        <label>protection of the grand witch</label>
        <description>The grand witch has cast powerful spells to protect herself.</description>
        <hediffClass>HediffWithComps</hediffClass>
        <defaultLabelColor>(199,213,231)</defaultLabelColor>
        <isBad>false</isBad>
        <everCurableByItem>false</everCurableByItem>
        <stages>
            <li>
                <statOffsets>
                    <PsychicSensitivity>-10.0</PsychicSensitivity>
                    <ToxicResistance>1</ToxicResistance>
                    <ToxicEnvironmentResistance>1</ToxicEnvironmentResistance>
                    <VacuumResistance MayRequire="Ludeon.RimWorld.Odyssey">1</VacuumResistance>
                    <ComfyTemperatureMin>-273</ComfyTemperatureMin>
                    <ComfyTemperatureMax>2000</ComfyTemperatureMax>
                </statOffsets>
            </li>
        </stages>
        <comps>
            <li Class="UranvManosaba.Contents.Comps.HediffCompProperties_NarehateHealingReveal">
                <applyPostMultiplier>true</applyPostMultiplier>
            </li>
        </comps>
    </HediffDef>
